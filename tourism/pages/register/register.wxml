<!--pages/register/register.wxml-->
<view class='container'>
  <van-cell-group>
    <van-field
      value="{{ realname }}"
      label="真实姓名"
      placeholder="请输入真实姓名"
      clearable
      required
    />
    <van-field
      value="{{ school }}"
      label="学校"
      placeholder="请输入您的学校"
      required
      border="{{ false }}"
    />
  </van-cell-group>
  <v-button content="提交" />
</view>