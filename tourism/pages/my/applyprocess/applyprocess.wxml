<!--pages/my/my.wxml-->
<view class="page">
  <!--主体-->
  <view class="page__bd">
    <view class="list">

      <view class="page__bd">
        <view class="weui-cells weui-cells_after-title">
          <text>出行计划</text>
          <text>{{info.plan}}</text>
          <text>申请人信息</text>
          <text>{{info.user}}</text>
          <button class="confirm-btn" bindtap='approve'>同意</button>
          <button class="error-btn" bindtap='reject'>拒绝</button>
        </view>
      </view>
    </view>
  </view>
</view>
